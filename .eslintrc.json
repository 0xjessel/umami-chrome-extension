{
  "env": {
    "browser": true,
    "es2021": true,
    "webextensions": true,
    "jest": true
  },
  "extends": [
    "eslint:recommended",
    "prettier"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-console": ["error", { "allow": ["error", "warn"] }],
    "no-unused-vars": "warn",
    "prefer-const": "error",
    "eqeqeq": "error"
  }
}
